<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="co.com.arrendamientosnutibara.main.CurrentRouteActiviy"
    tools:showIn="@layout/activity_route">

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:id="@+id/ownerships_recycler"
        android:layout_height="match_parent"/>

    <co.com.arrendamientosnutibara.widgets.CenturyBoldButton
        android:layout_width="match_parent"
        android:layout_marginTop="8dp"
        style="@style/Widget.AppCompat.Button.Colored"
        android:text="Ingresa"
        android:id="@+id/login_button"
        android:layout_height="wrap_content" />

</LinearLayout>
